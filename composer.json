{
    "name": "chirripo/chirripo",
    "description": "Chirripo is an open source docker containers setup to be added easily to any existing php project.",
    "keywords": ["chirripo", "docker", "containers"],
    "license": "MIT",
    "authors": [
        {"name": "Kevin Porras", "email": "kporras07@gmail.com"}
    ],
    "homepage": "https://github.com/chirripo/chirripo",
    "support": {
        "docs": "https://github.com/chirripo/chirripo"
    },
    "bin": ["bin/chirripo"],
    "require": {
        "symfony/console": "^3.4",
        "symfony/process": "^3.4",
        "symfony/dotenv": "^3.4",
        "consolidation/robo": "^1.4",
        "composer/composer": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Console\\": "src"
